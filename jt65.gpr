project JT65 is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("jt65code.adb");

   package Ide is
      for Default_Switches ("adacontrol") use ("-f", "jt65.aru");
   end Ide;

   package Compiler is
      for Switches ("ada") use ("-g", "-gnata", "-gnatn", "-gnatwa");
   end Compiler;

end JT65;

